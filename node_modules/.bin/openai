#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/오연수사무실/개인/공부/최종251120/node_modules/.pnpm/openai@6.8.1_ws@8.18.3_zod@3.25.76/node_modules/openai/bin/node_modules:/mnt/d/오연수사무실/개인/공부/최종251120/node_modules/.pnpm/openai@6.8.1_ws@8.18.3_zod@3.25.76/node_modules/openai/node_modules:/mnt/d/오연수사무실/개인/공부/최종251120/node_modules/.pnpm/openai@6.8.1_ws@8.18.3_zod@3.25.76/node_modules:/mnt/d/오연수사무실/개인/공부/최종251120/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/오연수사무실/개인/공부/최종251120/node_modules/.pnpm/openai@6.8.1_ws@8.18.3_zod@3.25.76/node_modules/openai/bin/node_modules:/mnt/d/오연수사무실/개인/공부/최종251120/node_modules/.pnpm/openai@6.8.1_ws@8.18.3_zod@3.25.76/node_modules/openai/node_modules:/mnt/d/오연수사무실/개인/공부/최종251120/node_modules/.pnpm/openai@6.8.1_ws@8.18.3_zod@3.25.76/node_modules:/mnt/d/오연수사무실/개인/공부/최종251120/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openai/bin/cli" "$@"
else
  exec node  "$basedir/../openai/bin/cli" "$@"
fi
